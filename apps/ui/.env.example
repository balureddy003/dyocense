# Dyocense API Base URL
VITE_DYOCENSE_BASE_URL=http://127.0.0.1:8001

# OAuth Callback Base URL (must match backend configuration)
# This is automatically used for OAuth redirects
# Production: https://yourdomain.com
# Development: http://localhost:5173

# Admin tenant ID (required for admin dashboard access)
VITE_ADMIN_TENANT_ID=admin

# Keycloak SSO Configuration (optional)
VITE_KEYCLOAK_URL=http://localhost:8080
VITE_KEYCLOAK_REALM=master
VITE_KEYCLOAK_CLIENT_ID=dyocense-ui

# Force Goal Planner to use client stub instead of API ("1" or "true")
VITE_GOAL_PLANNER_FORCE_STUB=1
