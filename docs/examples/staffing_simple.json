{
  "metadata": {
    "ops_version": "1.0.0",
    "problem_type": "staffing",
    "tenant_id": "t_demo",
    "project_id": "p_demo"
  },
  "objective": {
    "sense": "min",
    "expression": "sum(W[e]*hours[e] for e in E)"
  },
  "decision_variables": [
    {
      "name": "assign[e,t]",
      "type": "binary",
      "lb": 0,
      "ub": 1,
      "index_sets": [
        "E",
        "T"
      ]
    }
  ],
  "parameters": {
    "E": [
      "Alice",
      "Bob",
      "Cara"
    ],
    "T": [
      "Mon-AM",
      "Mon-PM",
      "Tue-AM",
      "Tue-PM"
    ],
    "demand": {
      "Mon-AM": 2,
      "Mon-PM": 1,
      "Tue-AM": 2,
      "Tue-PM": 1
    },
    "W": {
      "Alice": 12.0,
      "Bob": 10.0,
      "Cara": 9.5
    }
  },
  "constraints": [
    {
      "name": "cover[t]",
      "for_all": "T",
      "expression": "sum(assign[e,t] for e in E) >= demand[t]"
    }
  ],
  "kpis": [
    {
      "name": "total_hours",
      "expression": "sum(assign[e,t] for e in E for t in T)"
    }
  ],
  "validation_notes": []
}