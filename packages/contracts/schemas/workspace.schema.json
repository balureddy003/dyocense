{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.dyocense.com/workspace.schema.json",
  "title": "Workspace",
  "type": "object",
  "description": "Tenant-level workspace metadata surfaced to SMB clients.",
  "required": ["id", "name"],
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "industry": {
      "type": "string"
    },
    "region": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false
}
